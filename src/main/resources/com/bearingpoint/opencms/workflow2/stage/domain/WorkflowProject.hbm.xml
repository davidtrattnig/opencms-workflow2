<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.bearingpoint.opencms.workflow2.stage.domain">

    <class name="WorkflowProject" table="wf_project">
        <id name="id" column="ID" unsaved-value="null">
            <generator class="native" />
        </id>

        <property name="projectID" column="PROJECT_UUID" />
        <property name="stage" column="WORKFLOW_STAGE" />
    </class>

</hibernate-mapping>